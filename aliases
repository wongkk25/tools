#git
alias gita="git add"
alias gitamend="git commit --amend"
alias gitcout="git checkout"
alias gitc="git commit -m"
alias gitca="git commit ./ -m"
alias gitd="git diff"
alias gitl="git log"
alias gitp="git push"
alias gitpl="git pull"
alias gitstash="git stash"
alias gitsa="git stash apply"
alias gitsp="git stash pop"
alias gits="git status"

#mvn
alias mvni="mvn install"
alias mvnist="mvn install -DskipTests"
alias mvnistu="mvn install -DskipTests -U"
alias mvnci="mvn clean install"
alias mvncist="mvn clean install -DskipTests"
alias mvncistu="mvn clean install -DskipTests -U"
alias mvnt="mvn test"
alias mvndt="mvn dependency:tree"
alias mvnts="mvn test -DwildcardSuites=$1"
